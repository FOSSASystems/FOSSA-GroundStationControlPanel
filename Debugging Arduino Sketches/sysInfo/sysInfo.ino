void setup() {
  Serial.begin(9600);
}

void loop() {
  uint8_t sysInfo[] = {
    0x81, 0x25,  									  // 2 bytes
    'R', 'R', 										  // 2 bytes
    'F', 'O', 'S', 'S', 'A', 'S', 'A', 'T', '-', '2', // 10 bytes
    0x23, 0x17, 0x00, 0x83, 0xFF, 0x27, 0xD3, 0x1E,   // 8 bytes
    0x5F, 0x2B, 0x01, 0x00, 0x00, 0x00, 0x00, 0x00,   // 8 bytes
    0x00, 0xF9, 0x63, 0xFC, 0x09, 0x00, 0x00, 0x00,   // 8 bytes
    0x00										      // 1 byte
  };

  Serial.write(sysInfo, sysInfo[1] + 2);
  delay(3000);
}
